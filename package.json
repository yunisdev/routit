{
  "version": "1.0.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --runInBand",
    "lint": "tsdx lint",
    "lint-fix": "tsdx lint --fix",
    "prepare": "tsdx build",
    "shell": "node",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "json-server": "json-server --watch ./test/data/db.json --port=8000",
    "json-server-background": "pm2 --name JsonServer start \"npm run json-server\"",
    "json-server-background-stop":"pm2 delete JsonServer"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "routit",
  "author": "Yunis Huseynzade",
  "module": "dist/routit.esm.js",
  "size-limit": [
    {
      "path": "dist/routit.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/routit.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^5.0.3",
    "concurrently": "^6.2.1",
    "husky": "^7.0.1",
    "json-server": "^0.16.3",
    "pm2": "^5.1.1",
    "size-limit": "^5.0.3",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1",
    "typescript": "^3.9.10"
  },
  "dependencies": {
    "axios": "^0.21.1"
  }
}
